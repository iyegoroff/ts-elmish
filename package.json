{
  "version": "5.1.2",
  "name": "ts-elmish",
  "private": true,
  "files": [],
  "scripts": {
    "postinstall": "npm run fix-links",
    "preversion": "npm version $npm_new_version -ws && git commit -am up && npm run fix-links && npm run lint && npm run build && npm run check && npm t && npm run type-coverage",
    "postversion": "git push && git push --tags",
    "lint": "eslint . && npm run lint -ws --if-present",
    "build": "npm run build -ws --if-present",
    "check": "npm run check -ws --if-present",
    "test": "jest",
    "fix-links": "node ./scripts/fix-links",
    "patch-manifests": "glob-json 'packages/*/package.json' merge '' publishConfig && glob-json 'packages/*/package.json' del publishConfig && glob-json 'packages/*/package.json' set '+(devDependencies|dependencies|peerDependencies)/@ts-elmish/*' $npm_package_version",
    "type-coverage": "type-coverage --strict --detail --ignore-files **/dist/* --ignore-files **/blueprint-templates/**/* --ignore-catch"
  },
  "typeCoverage": {
    "atLeast": 100
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@types/jest": "^27.5.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-jest": "^27.5.1",
    "babel-plugin-spectypes": "^2.1.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-functional": "^3.7.2",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-react": "^7.29.4",
    "glob": "^8.0.3",
    "glob-json": "^0.0.2",
    "jest": "^27.5.1",
    "prettier": "^2.7.1",
    "runtypes": "^6.5.1",
    "type-coverage": "^2.21.1",
    "typescript": "^4.7.3"
  },
  "workspaces": [
    "./packages/*",
    "./examples/*",
    "./configs"
  ]
}

{
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "preset-version": "pnpm lint -r -- --fix && pnpm build -r && pnpm check -r && pnpm t && pnpm type-coverage",
    "set-version": "git add -u && git commit -am lint || : && pnpm -r update-version",
    "postset-version": "tag=v\"$(dot-json packages/core/package.json version)\" && git commit -am \"$tag [skip ci]\" && git tag $tag && git push && git push --tags",
    "lint": "eslint .",
    "test": "jest",
    "coverage": "codecov",
    "type-coverage": "type-coverage --strict --detail --ignore-files **/dist/* --ignore-files **/blueprint-templates/**/* --ignore-catch"
  },
  "typeCoverage": {
    "atLeast": 100
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-jest": "^27.5.1",
    "babel-plugin-spectypes": "^0.0.22",
    "dot-json": "^1.2.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-functional": "^3.7.2",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-react": "^7.29.4",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "runtypes": "^6.5.1",
    "ts-jest": "^27.1.4",
    "type-coverage": "^2.21.1",
    "typescript": "^4.6.3"
  }
}

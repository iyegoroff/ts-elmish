{
  "name": "@ts-elmish/eslint-plugin",
  "version": "0.0.61",
  "private": false,
  "description": "Eslint-based codegen",
  "main": "src/index.ts",
  "publishConfig": {
    "main": "dist/index.js",
    "types": "dist/index.d.ts"
  },
  "scripts": {
    "update-version": "npm version",
    "build:dts": "tsc -p ./source -d true --emitDeclarationOnly true --removeComments false",
    "build:esnext": "tsc -p ./source",
    "build": "rm -rf dist && mkdir dist && npm run build:dts && npm run build:esnext",
    "test": "mocha -r ts-node/register -t 7500 tests/**/*.ts"
  },
  "keywords": [
    "elmish",
    "typescript"
  ],
  "files": [
    "dist",
    "src"
  ],
  "bugs": {
    "url": "https://github.com/iyegoroff/ts-elmish/issues"
  },
  "homepage": "https://github.com/iyegoroff/ts-elmish#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iyegoroff/ts-elmish.git"
  },
  "author": "iyegoroff <iegoroff@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@typescript-eslint/experimental-utils": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "change-case": "^4.1.1",
    "eslint-etc": "^4.0.0",
    "requireindex": "^1.2.0",
    "ts-is-defined": "^1.0.0"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.0",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.10",
    "common-tags": "^1.8.0",
    "mocha": "^8.2.0",
    "ts-node": "^9.0.0"
  }
}

{
  "name": "@ts-elmish/core",
  "version": "5.0.4",
  "private": false,
  "description": "Elmish architecture in Typescript",
  "sideEffects": false,
  "main": "src/index.ts",
  "publishConfig": {
    "main": "./dist/cjs/index.cjs",
    "module": "./dist/esm/index.js",
    "react-native": "./dist/esm/index.js",
    "types": "./dist/dts/index.d.ts",
    "exports": {
      ".": {
        "require": "./dist/cjs/index.cjs",
        "import": "./dist/esm/index.js"
      },
      "./package.json": "./package.json"
    },
    "type": "module"
  },
  "scripts": {
    "update-version": "npm version",
    "build:dts": "tsc -p node_modules/@ts-elmish/configs/tsconfig.dts.json",
    "build:esm": "tsc -p node_modules/@ts-elmish/configs/tsconfig.esm.json",
    "build:cjs": "tsc -p node_modules/@ts-elmish/configs/tsconfig.cjs.json && mv dist/cjs/index.js dist/cjs/index.cjs",
    "build": "rm -rf dist && mkdir dist && npm run build:dts && npm run build:esm && npm run build:cjs"
  },
  "keywords": [
    "elmish",
    "typescript"
  ],
  "files": [
    "dist",
    "src"
  ],
  "bugs": {
    "url": "https://github.com/iyegoroff/ts-elmish/issues"
  },
  "homepage": "https://github.com/iyegoroff/ts-elmish#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iyegoroff/ts-elmish.git"
  },
  "author": "iyegoroff <iegoroff@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@ts-elmish/common": "file:packages/common"
  },
  "devDependencies": {
    "@ts-elmish/configs": "file:configs"
  }
}

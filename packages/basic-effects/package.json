{
  "name": "@ts-elmish/basic-effects",
  "version": "4.0.6",
  "private": false,
  "description": "Basic effects for ts-elmish",
  "sideEffects": false,
  "main": "src/index.ts",
  "publishConfig": {
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.js",
    "types": "./dist/dts/index.d.ts",
    "exports": {
      ".": {
        "require": "./dist/cjs/index.js",
        "import": "./dist/esm/index.js"
      },
      "./package.json": "./package.json"
    }
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "scripts": {
    "update-version": "npm version",
    "build:dts": "tsc -p node_modules/@ts-elmish/configs/tsconfig.dts.json",
    "build:esm": "tsc -p node_modules/@ts-elmish/configs/tsconfig.esm.json",
    "build:cjs": "tsc -p node_modules/@ts-elmish/configs/tsconfig.cjs.json",
    "build": "rm -rf dist && mkdir dist && npm run build:dts && npm run build:esm && npm run build:cjs"
  },
  "keywords": [
    "elmish",
    "typescript"
  ],
  "files": [
    "dist",
    "src"
  ],
  "bugs": {
    "url": "https://github.com/iyegoroff/ts-elmish/issues"
  },
  "homepage": "https://github.com/iyegoroff/ts-elmish#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iyegoroff/ts-elmish.git"
  },
  "author": "iyegoroff <iegoroff@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@ts-elmish/common": "workspace:*"
  },
  "devDependencies": {
    "@ts-elmish/configs": "workspace:*"
  }
}

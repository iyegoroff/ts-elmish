{
  "compilerOptions": {
    "sourceMap": true,
    "target": "es6",
    "strict": true,
    "jsx": "react",
    "lib": ["es2016", "dom"],
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "noEmitOnError": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "removeComments": true,
    "types": []
  }
}
